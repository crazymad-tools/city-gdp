{
  "defaultSeverity": "error",
  "extends": ["tslint-react", "tslint-eslint-rules"],
  "jsRules": {
      "rules": {
          "eofline": true,
          "no-console": true,
          "no-construct": true,
          "no-debugger": true,
          "no-reference": true,
          "indent": [true, "spaces", 4],
          "jsx-no-multiline-js": false
      }
  },
  "rules": {
      "jsx-no-multiline-js": false,
      "jsx-no-lambda": false
  }
}
